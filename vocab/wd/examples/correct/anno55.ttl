<span class="w">    </span><span class="nn">oa:</span><span class="nt">hasBody</span><span class="w"> </span><span class="nv">&lt;http://example.org/vocab/art/portrait&gt;</span><span class="w"> </span><span class="p">;</span><span class="w"></span>
<span class="w">    </span><span class="nn">oa:</span><span class="nt">hasTarget</span><span class="w"> </span><span class="p">[</span><span class="w"></span>
<span class="w">        </span><span class="kt">a</span><span class="w"> </span><span class="nn">oa:</span><span class="nt">Independents</span><span class="w"> </span><span class="p">;</span><span class="w"></span>
<span class="w">        </span><span class="nn">as:</span><span class="nt">items</span><span class="w"> </span><span class="p">(</span><span class="w"></span>
<span class="w">          </span><span class="nv">&lt;http://example.com/image1&gt;</span><span class="w"> </span>
<span class="w">          </span><span class="nv">&lt;http://example.net/image2&gt;</span><span class="w"></span>
