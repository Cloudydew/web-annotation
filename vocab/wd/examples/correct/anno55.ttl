<span class="w">      </span><span class="nn">dc:</span><span class="nt">language</span><span class="w"> </span><span class="s">&quot;en&quot;</span><span class="p">,</span><span class="w"> </span><span class="s">&quot;he&quot;</span><span class="w"> </span><span class="p">;</span><span class="w"></span>
