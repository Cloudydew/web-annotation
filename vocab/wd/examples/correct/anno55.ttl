    oa:hasBody [ 
        a oa:TextualBody ;
        rdf:value "A reply to a question" ] ;
    oa:hasTarget &lt;http://example.com/anno1&gt; ;
    oa:motivatedBy oa:replying .
    </pre>
  </div>
</section>


